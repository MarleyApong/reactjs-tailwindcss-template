{
  "name": "reactjs-tailwindcss-template",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev:vite": "vite",
    "watch:routes": "tsx scripts/watch-routes.ts",
    "dev": "npm-run-all --parallel dev:vite watch:routes",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "preview": "vite preview",
    "release:dev": "standard-version --prerelease dev",
    "release:prod": "standard-version",
    "parse": "node scripts/i18n/i18n-parser.js",
    "parse:verbose": "node scripts/i18n/i18n-parser.js --verbose",
    "parse:sort": "node scripts/i18n/i18n-parser.js --sort",
    "parse:clean": "node scripts/i18n/i18n-parser.js --clean",
    "parse:all": "node scripts/i18n/i18n-parser.js --verbose --sort --clean",
    "clean": "node scripts/clean.js",
    "clean:install": "node scripts/clean.js && npm install"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/react-router": "^1.134.20",
    "chokidar": "^4.0.3",
    "fs-extra": "^11.3.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "standard-version": "^9.5.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  }
}
